{
    "name": "mindtwo/lumen-wp-app",
    "type": "library",
    "description": "This package contains a application file to use the Laravel-Lumen service container with some Laravel components inside of WordPress.",
    "keywords": [
        "wordpress",
        "laravel lumen",
        "lumen"
    ],
    "homepage": "https://github.com/mindtwo/lumen-wp-app",
    "license": "MIT",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "vlucas/phpdotenv": "^4.0",
        "symfony/var-dumper": "^5.0",
        "symfony/error-handler": "^5.0",
        "illuminate/container": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/translation": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/encryption": "^7.0",
        "illuminate/cache": "^7.0",
        "illuminate/config": "^7.0",
        "illuminate/log": "^7.0",
        "illuminate/events": "^7.0",
        "illuminate/contracts": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LumenWpApp\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
