{
    "name": "mindtwo/lumen-wp-app",
    "type": "library",
    "description": "This package contains a application file to use the Laravel-Lumen service container with some Laravel components inside of WordPress.",
    "keywords": [
        "wordpress",
        "laravel lumen",
        "lumen"
    ],
    "homepage": "https://github.com/mindtwo/lumen-wp-app",
    "license": "MIT",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.1",
        "vlucas/phpdotenv": "^5.4.1",
        "symfony/var-dumper": "^6.1",
        "symfony/error-handler": "^6.1",
        "illuminate/container": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/translation": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/validation": "^10.0",
        "illuminate/encryption": "^10.0",
        "illuminate/hashing": "^10.0",
        "illuminate/cache": "^10.0",
        "illuminate/config": "^10.0",
        "illuminate/log": "^10.0",
        "illuminate/events": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/http": "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LumenWpApp\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
