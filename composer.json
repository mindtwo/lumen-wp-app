{
    "name": "mindtwo/lumen-wp-app",
    "type": "library",
    "description": "XXX",
    "keywords": [
        "wordpress",
        "laravel lumen",
        "lumen"
    ],
    "homepage": "https://github.com/mindtwo/lumen-wp-app",
    "license": "MIT",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "vlucas/phpdotenv": "^3.6",
        "symfony/var-dumper": "^4.2",
        "illuminate/container": "^6.0",
        "illuminate/support": "^6.0",
        "illuminate/translation": "^6.0",
        "illuminate/filesystem": "^6.0",
        "illuminate/encryption": "^6.0",
        "illuminate/cache": "^6.0",
        "illuminate/config": "^6.0",
        "illuminate/log": "^6.0",
        "illuminate/contracts": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LumenWpApp\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
